#include<stdio.h>
#include<stdlib.h>

struct Stack {
    int top;
    int size;
    int* arr;
};
int main(){
    // Create a stack of size 5
    struct Stack* s = (struct Stack*)malloc(sizeof(struct Stack));
    s->size=5;
    s->top =-1;
    s->arr=(int*)malloc(s->size * sizeof(int));
    // Push elements onto the stack
    for(int i=0;i<s->size ; i++){
        s->top++;
        s->arr[s->top] = i+3;
        printf("Pushed %d onto stack\n",s->arr[s->top]);
    }
    // Pop elements from the stack
    for(int i =0; i<s->size ; i++){
        printf("Popped element %d is : %d\n",i+1,s->arr[s->top]);
        s->top--;
    }
    return 0;
}